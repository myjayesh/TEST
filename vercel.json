{
  "functions": {
    "src/pages/api/**/*.ts": {
      "runtime": "@vercel/node@3.0.31"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/pages/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|_next/|static/|public/|favicon.ico|.*\\\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "outputDirectory": "dist"
}
