{
  "functions": {
    "api/**/*.ts": {  <-- THIS LINE HAS CHANGED
      "runtime": "@vercel/node@3.0.31"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/((?!api/|_next/|static/|public/|favicon.ico|.*\\\\..*).*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": "dist"
}
